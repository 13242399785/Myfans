/* pages/checkin/checkin.wxss */
.checkin-tab{
  display: flex;
  background-color: #fff;
  height: 88rpx;
  position: relative;
  border-bottom: 2rpx solid rgba(0, 0, 0, .2);
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 99;
}
.checkin-tab-item{
  width: 33.33%;
  font-size: 28rpx;
  text-align: center;
  line-height: 88rpx;
}
.checkin-content{
  padding-top:88rpx;
}
.checkin-tab-active{
  color: rgb(255, 214, 0);
}
.checkin-bar{
  width: 33.33%;
  text-align: center;
  position: absolute;
  height: 4rpx;
  left: 0;
  transition: all ease .5s;
  bottom: 0;
}
.checkin-barlist{
  width: 52rpx;
  height: 4rpx;
  display: inline-block;
  background: rgb(255, 214, 0);
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}
.checkin-content{

}
.checkin-daka-wapper{
  width: 94%;
  margin: 1rem auto;
}

.gohome{
  position: fixed;
  bottom: 0;
  width: 100%;
  max-width: 42.85714rem;
  height: 3.21429rem;
  color: #202020;
  background: #fff;
  font-size: 1.07143rem;
  font-family: sans-serif;
  line-height: 3.21429rem;
  text-align: center;
}
.gohome .iconfont{
  font-size:34rpx;
}
.rili{
  background:#fff;
  position: relative;
  
}
.rili .iconfont{font-size: 50rpx;}
/* 排名 */
.checkin-raking{
  color: #444;
  font-size: 0.85714rem;
}
.ranking-top{
  position: relative;
  width: 100%;
  height: 23.57143rem;
  font-size: 1rem;
  background: url('https://cdn.myfans.cc/new-home/img/rankbg.0bb2530c.png') 0 0 no-repeat / 100% 100%;
}
.ranking-btn{
  position: relative;
  top: 1.42857rem;
  width: 11.42857rem;
  height: 2.57143rem;
  margin: 0 auto;
  overflow: hidden;
  line-height: 2.57143rem;
  border: 2rpx solid #fff;
  border-radius: 120rpx;
}
.ranking-btn-list{
  display: block;
  float: left;
  width: 50%;
  color: #fff;
  background: transparent;
  text-align: center;
}
.ranking-btn-link{
  color: #000;
  background: #fff;
}
.ranking-first{
  width: 4.28571rem;
  text-align: center;
  position: absolute;
  left: 50%;
  top: 5.5rem;
  transform: translateX(-50%);
}
.ranking-img{
  width: 4.28571rem;
  height: 4.28571rem;
  margin-bottom: 0.57143rem;
}
.rankingNum{
  position: absolute;
  top: 3.57143rem;
  left: 1.42857rem;
  z-index: 100;
  display: block;
  width: 1.42857rem;
  height: 1.42857rem;
  font-size: 1rem;
  line-height: 1.42857rem;
  text-align: center;
  background-color: #fff;
  border-radius: 50%;
}
.first-num{
  top: 3.57143rem;
  left: 1.42857rem;
}
.first_bottom{
  position: absolute;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  bottom: 5.35714rem;
}
.ranking-second{
  text-align: center;
  top: 8.5rem;
  position: absolute;
  left: 17%;
  transform: translateX(-50%);
}
.rankimg{
  margin-bottom: 0.57143rem;
  border: 2rpx solid #fff;
  border-radius: 50%;
}
.second_bottom{
  bottom: 1.78571rem;
  position: absolute;
  left: 17%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
}
.ranking-third{
  position: absolute;
  left: 83%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  white-space:nowrap;
}
/* list */
.ranking-list{
  background: #fff;
}
.ranking-list::before{
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 2rpx;
  border-top: 2rpx solid #D9D9D9;
  color: #D9D9D9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}
.ranking-item{
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  align-items: center;

}
.ranking-item::after{
  position: absolute;
  content:'';
  height: 2rpx;
  bottom: 0;
  width: 94%;
  right: 0;
  background: #ccc;
}
.user-rank{
  display: inline-block;
  width: 2.35714rem;
  height: 2.5rem;
  overflow: hidden;
  color: #ffc042;
  font-weight: 700;
  font-size: 1.64286rem;
  line-height: 2.92857rem;
  text-align: center;
  text-overflow: ellipsis;
  vertical-align: top;
  word-wrap: break-word;
}
.ranking-item-left{
  display: flex;
}
.ranking-item-center{
  flex: 1;
}
.ranking-item-right{
  text-align: right;
  color: #999999;
}


.user-rankimg{
  position: relative;
  top: 0.21429rem;
  width: 2.5rem;
  height: 2.5rem;
  margin-right: 0.5rem;
}
.ranking-hot-wapper{
  margin-bottom: 3.92857rem;
}
.my-integral{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 3.92857rem;
  color: #444;
  font-size: 1rem;
  line-height: 3.92857rem;
  background: #fff;
  border-top: 1px solid #ddd;
}
.my-img{
  width: 2.85714rem;
  height: 2.85714rem;
  margin: 0.53571rem 1.1rem;
  display: inline-block;
}
.integral-name{
  position: absolute;
}
.my-cishu{
  float: right;
  padding-right: 1rem;
}
/*  */
.calendar_title{width: 70%; margin: 10rpx auto; justify-content: space-between;font-size: 16pt;color: #ccc;}
.calendar_title .icon image{width: 60rpx; height: 60rpx;margin: 5rpx auto;}

/* 日历 */
.calendar{width: 100%;margin-top: 20rpx;background: #fff;border-bottom-right-radius: 20rpx;border-bottom-left-radius: 20rpx;padding: 0 0 20rpx;font-family: FZZhunYuan-M02S;}
.header{font-size: 0;width: 90%;margin: 0 auto;}
.header>view{display: inline-block;width: 14.285%;color: #666;font-size: 30rpx;text-align: center;border-bottom: 1px solid #D0D0D0;padding: 20rpx 0;}
.weekMark{position: relative;width: 90%;margin: 0 auto;}
.weekMark view{position: absolute;bottom: 0;left: 0;width: 100%;border-bottom: 2px solid #69f;}
.date-box{font-size: 0;padding: 10rpx 0;width: 90%;margin: 0 auto;}
.date-box>view{position: relative;display: inline-block;width: 14.285%;color: #666;text-align: center;vertical-align: middle;}
.date-head{height: 60rpx;line-height: 60rpx;font-size: 12pt;}
.nowDay .date-head{width: 60rpx;border-radius: 50%;text-align: center;color: #fff;background-color: #ff9933;margin: 0 auto;}
.flex_nowrap{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
  padding: 1.42857rem 0;
}
.diandian{
  width: 10rpx;
  height: 10rpx;
  display: inline-block;
  position: absolute;
  background: red;
  border-radius: 50%;
  left: 50%;
  transform: translateX(-50%);
  bottom: -3rpx;
}
.leiji{
  text-align: center;
  color: rgb(113, 113, 113);
  font-size: 14px;
}
.pleiji{
  margin-top: 2rem;
}